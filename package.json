{
    "name": "core-property-management",
    "description": "Core Property Management",
    "repository": {
        "type": "git",
        "url": "https://github.com/redisrupt/red"
    },
    "private": true,
    "version": "0.0.1",
    "main": "bin/server.js",
    "bnrConfig": "./tasks.js",
    "scripts": {
        "auth:start": "bnr auth:start --production",
        "auth:start-dev": "bnr auth:start",
        "auth:start-dev-cucumber": "bnr auth:start --cucumber",
        "autofix": "bnr autofix",
        "autofix-no-untracked": "bnr autofix --include-others=false",
        "autofix-all": "npm run beautify && npm run lint-fix",
        "beautify": "npm run beautify-css",
        "beautify-check": "bnr csslint",
        "beautify-css": "bnr csslint --fix",
        "beautify-css-check": "npm run beautify-css -- -k",
        "beautify-js": "esbeautifier 'client/**/*.js' 'bin/**/*.js' 'server/**/*.js' 'common/**/*.js' 'webpack/**/*.js' '*.js'",
        "beautify-js-check": "npm run beautify-js -- -k",
        "build": "npm run verify --silent && npm run do-build",
        "build-client-dev": "npm run gen-sprite && npm run gen-demo-routes && babel-node --extensions '.ts,.js,.json' ./node_modules/webpack/bin/webpack --stats-error-details --config webpack/webpack-config.js --bail",
        "bump-major": "npm run pre-v && npm version major -m 'BLD: Release v%s' && npm run post-v",
        "bump-minor": "npm run pre-v && npm version minor -m 'BLD: Release v%s' && npm run post-v",
        "bump-patch": "npm run pre-v && npm version patch -m 'BLD: Release v%s' && npm run post-v",
        "changelog": "changelogx -f markdown -o ./changelog.md",
        "check": "bnr check",
        "check-files": "bnr check-files",
        "client-unit-test": "npm run gen-sprite && npm run jest",
        "client-unit-test:watch": "npm run jest -- --watch",
        "compress": "bnr compress",
        "_compress": "babel-node resources/compress.js --manifestDir ",
        "compress:auth": "npm run _compress -- ./auth/static/dist/",
        "compress:red": "npm run _compress -- ./static/dist/",
        "compress:consumer": "npm run _compress -- ./consumer/static/dist/",
        "compress:resexp": "npm run _compress -- ./resexp/static/dist/",
        "cucumber-local": "bnr cucumber-local",
        "cucumberjs": "bnr cucumberjs",
        "cucumber-report-test": "bnr cucumber-report-test",
        "cucumber-report-local": "bnr cucumber-report-local",
        "debug": "TZ=UTC shell-exec --bail 'bnr start-debug' 'bnr start-debug-api' 'bnr start-debug-worker' 'bnr start-debug-socket'",
        "dev": "bnr app-start --socket --leasing --api --worker --decision_api --export_api",
        "dev:all": "bnr app-start --all",
        "dev-cucumber": "bnr app-start --all --cucumber",
        "dev-components": "TZ=UTC bnr dev-components",
        "dist-clean": "rm -rf report/ static/dist/ auth/static/dist/ consumer/static/dist/ resexp/static/dist/ cucumber/output/ static/dev-dist/ auth/static/dev-dist/ consumer/static/dev-dist/ resexp/static/dev-dist/ ./.stats/",
        "do-build": "npm run dist-clean && npm run gen-sprite && npm run gen-demo-routes && bnr do-build && bnr checkBundles && npm run compress",
        "build-and-cache-bust": "bnr do-build",
        "do-build-module": "bnr do-build-module",
        "do-changelog": "npm run changelog && git add ./changelog.md && git commit -m 'DOC: Generate changelog' --no-verify",
        "gen-demo-routes": "babel-node resources/demo-routes.js",
        "gen-sprite": "babel-node resources/svg-sprite.js",
        "generate-cucumber-report": "babel-node --extensions '.ts,.js,.json' ./resources/report-cucumber.js",
        "gherkin-lint": "gherkin-lint cucumber/",
        "install-hooks": "changelogx install-hook",
        "integration-test": "npm run integration-test-all-modules",
        "integration-test-all-modules": "npm run integration-test-single -- './server/import/__integration__/data-import-test.js' './server/export/__integration__/database/databaseExport-test.js' '{.,rentapp,resexp,roommates,auth,resident}/server/**/__integration__/**/*-test.js' './common/test-helpers/__integration__/*-test.js'",
        "integration-report": "npm run integration-test-all-modules -- --reporter mocha-multi-reporters --reporter-options configFile=./mochaConfig.json",
        "integration-test-report": "babel-node --extensions '.ts,.js,.json' resources/integration-runner ",
        "integration-test-silent": "RED_LOGGER_USE_STDOUT=false npm run integration-test",
        "integration-test-single": "bnr exec-integration-test ",
        "jest": "BABEL_CACHE_PATH=node_modules/.cache/babel/jest.json FORCE_COLOR=true TZ=UTC node --max-old-space-size=4096 node_modules/.bin/jest --notify --logHeapUsage --detectOpenHandles --config jest-client.js",
        "jest-server": "BABEL_CACHE_PATH=node_modules/.cache/babel/jest-server.json FORCE_COLOR=true TZ=UTC node --max-old-space-size=4096 node_modules/.bin/jest --notify --logHeapUsage --detectOpenHandles --config jest-server.js",
        "jest-server:watch": "npm run jest-server -- --watch",
        "jest:watch": "npm run jest -- --watch",
        "lint": "bnr lint",
        "lint-files": "babel-node resources/lint-files",
        "lint-fix": "npm run lint -- --fix ",
        "post-v": "npm run do-changelog && git push --no-verify && git push --tags --no-verify",
        "pre-v": "npm run verify",
        "consumer:start": "bnr consumer:start --production",
        "consumer:start-dev": "bnr consumer:start",
        "consumer:start-dev-cucumber": "bnr consumer:start --cucumber",
        "resexp:start": "bnr resexp:start --production",
        "resexp:start-dev": "bnr resexp:start",
        "resexp:start-dev-cucumber": "bnr resexp:start --cucumber",
        "server-unit-test": "npm run jest-server",
        "server-unit-test:watch": "npm run jest-server:watch",
        "sonar-scanner": "bnr sonar-scanner",
        "start": "bnr app-start --socket --leasing --api --worker --decision_api --export_api --production",
        "start:leasing": "bnr app-start --production --skipHealthCheck --leasing",
        "start:consumer": "bnr app-start --production --skipHealthCheck --consumer",
        "start:auth": "bnr app-start --production --skipHealthCheck --auth",
        "start:api": "bnr app-start --production --skipHealthCheck --api",
        "start:socket": "bnr app-start --production --skipHealthCheck --socket",
        "start:decision_api": "bnr app-start --production --skipHealthCheck --decision_api",
        "start:export_api": "bnr app-start --production --skipHealthCheck --export_api",
        "start:worker": "bnr app-start --production --worker",
        "start:all": "bnr app-start --all --production --skipHealthCheck",
        "start-integration": "./bnr leasing:start --integration",
        "test": "bnr test",
        "total-coverage": "rm -rf report coverage && npm test && echo TOTAL COVERAGE REPORT && ./node_modules/.bin/istanbul report --root ./ --include **/*coverage*.json text-summary",
        "validate-apidoc": "bnr validate-apidoc",
        "verify": "node resources/check-configs.js && bnr verify",
        "watch-test": "NODE_ENV=cov watch-spawn -p './server/**/*.js' -p './common/**/*.js' -i -d 300 babel-node node_modules/.bin/_mocha ",
        "worker-dev": "bnr worker:start",
        "storybook": "start-storybook -p 6006 -c ./storybook -s ./static",
        "build:prepare": "npm run gen-sprite && npm run gen-demo-routes",
        "backend:start": "nodemon --signal SIGHUP --delay 3000ms --ignore 'cucumber' --ignore 'testcafe' --ignore 'images' --ignore 'scripts' --ignore 'vs-config' --ignore 'client' --ignore 'local_modules' --ignore 'static' --ignore 'upload' --ignore 'rentapp/client' --ignore 'docs' --ignore 'docker' --ignore 'trans' --ignore 'webpack' --ignore 'roommates/client' --ignore 'storybook' --ignore 'stories' --exec 'npm run backend:start:dev'",
        "backend:start:dev": "bnr app-start --backend --skipHealthCheck",
        "frontend:start": "bnr app-start --frontend --noDemo --skipVendors --skipHealthCheck",
        "frontend:start:watch": "bnr app-start --frontend --noDemo --skipVendors --skipHealthCheck --watch",
        "frontend:server-only": "npm run frontend:start -- --skipDevServer",
        "build-storybook": "build-storybook -c ./storybook",
        "postinstall": "patch-package || true",
        "generate-api-json-files": "babel-node --extensions '.js,.ts' resources/bin/generate-require-from-actions.js"
    },
    "precommit": [
        "npm run check --silent"
    ],
    "prepush": [
        "npm run verify --silent"
    ],
    "changelogx": {
        "ignoreRegExp": [
            "BLD: Release",
            "DOC: Generate Changelog",
            "Generated Changelog"
        ],
        "issueIDRegExp": "#(\\d+)",
        "commitURL": "https://github.com/Redisrupt/red/commit/{0}",
        "authorURL": "https://github.com/{0}",
        "issueIDURL": "https://github.com/Redisrupt/red/issues/{0}",
        "projectName": "red"
